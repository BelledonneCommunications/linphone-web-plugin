<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:name>${FBSTRING_PluginName}</em:name>
    <em:version>${FBSTRING_PLUGIN_VERSION}</em:version>
    <!-- <em:bootstrap>true</em:bootstrap> -->
    <em:id>${FBControl_XPI_GUID}</em:id>
    <em:type>2</em:type>
    <em:description>${FBSTRING_FileDescription}</em:description>

	<em:icon64URL>chrome://${PLUGIN_SHAREDIR}/skin/icon64.png</em:icon64URL>
	<em:iconURL>chrome://${PLUGIN_SHAREDIR}/skin/icon48.png</em:iconURL>
	
    <em:targetApplication>
      <!-- Firefox -->
      <Description>
       <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
       <em:minVersion>1.5</em:minVersion>
       <em:maxVersion>24.0</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:targetPlatform>WINNT</em:targetPlatform>

    <em:creator>${FBSTRING_CompanyName}</em:creator>
    <em:developer>Yann Diorcet</em:developer>
    <em:unpack>true</em:unpack>
  </Description>
</RDF>
